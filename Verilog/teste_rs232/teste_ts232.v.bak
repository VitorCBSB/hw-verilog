module teste_ts232(iCLK_50, iKEY, iLEDR, iSW, iUART_RXD, oUART_TXD);

	input iCLK_50;
	input iKEY[3:0];
	output [17:0] iLEDR;
	input [17:0] iSW;
	input iUART_RXD;
	output oUART_TXD;
	
	assign iLEDR[8] = oUART_TXD;
	assign iLEDR[9] = iUART_RXD;
	
serialGPIO rs232(
	.clk(iCLK_50),
	.RxD(iUART_RXD),
	.TxD(oUART_TXD),
	
	.GPout(iLEDR[7:0]),
	.GPin(iSW[7:0])
);

endmodule