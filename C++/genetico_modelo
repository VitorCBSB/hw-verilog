module genetico(conf_les, conf_outs, in, out);

	input [#r_x_c:0][#tam_le:0] conf_les;
	input [#num_out:0][#bits_pinos:0] conf_outs;
	input [#num_in:0] in;
	output [#num_out:0] out;
	
	wire [#r_x_c:0] le_out;
	wire [#num_pinos:0] all_inputs;
	
	assign all_inputs = {le_out, in};
	
	assign out = {#all_inputs_for_out};
	
#les

endmodule
